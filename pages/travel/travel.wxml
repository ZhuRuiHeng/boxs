<!--index.wxml-->
<view  wx:if="{{luggageInform==false}}" style="text-align:center;padding:30rpx 0;font-size:32rpx;">
     <view class="classname">你还没有物品分类，快去新建~~</view>
</view>
<view class="container" wx:else>
    <block>
        <view class='header'> 服饰 </view> 
        <view class='item'>
            <view class='logo del'><image src="../image/rol.png" mode="widthFix"></image></view>
            <view class='logo '><image src="../image/rols.png" mode="widthFix"></image></view>
            <view class='content'>
                <view class='text'>
                    <view class='title'>内容</view>  
                </view>
            </view>
            <view class='set' bindtap='setting'><image src="../image/setting.png" mode="widthFix"></image></view>
        </view>
    </block>
</view>
<view class='footer'>
    <image src="../image/add.png" mode='widthFix' bindtap='addNew'></image>
</view>
